                            disp('Total (including 7% tax):');
                            disp(sprintf('$%5.2f',akroger));
                        case 4
                            loop2 = OFF;
                            close;
                    end
                case 4
                    fprintf('Subtotal of cheapest items between the 3 stores: $%5.2f\n',b);
                    choice2 = menu('Please enter tax %','5%','6%','7%','done');
                    switch choice2
                        case 1
                            c=b+(b*0.05);
                            disp('Total (including 5% tax):');
                            disp(sprintf('$%5.2f',c));
                        case 2
                            c=b+(b*0.06);
                            disp('Total (including 6% tax):');
                            disp(sprintf('$%5.2f',c));
                        case 3
                            c=b+(b*0.07);
                            disp('Total (including 7% tax):');
                            disp(sprintf('$%5.2f',c));
                        case 4
                            loop2 = OFF;
                            close;
                    end
            end
    end
     
