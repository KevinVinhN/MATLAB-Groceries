        case 46
            loop = OFF;
            close;
            b=sum(a);
            choice15 = menu('Please choose one of the following: ','Walmart only','Publix only', 'Kroger only', 'Cheapest between all 3');
            switch choice15
                case 1
                    fprintf('Subtotal of items strictly from Walmart only: $%5.2f\n', awalmart);
                    choice3 = menu('Please enter tax %','5%','6%','7%','done');
                    switch choice3
                        case 1
                            awalmart = awalmart + (awalmart*0.05);
                            disp('Total (including 5% tax):');
                            disp(sprintf('$%5.2f',awalmart));
                        case 2
                            awalmart = awalmart + (awalmart*0.06);
                            disp('Total (including 6% tax):');
                            disp(sprintf('$%5.2f',awalmart));
                        case 3
                            awalmart = awalmart + (awalmart*0.07);
                            disp('Total (including 7% tax):');
                            disp(sprintf('$%5.2f',awalmart));
                        case 4
                            loop2 = OFF;
                            close;
                    end
                case 2
                    fprintf('Subtotal of items strictly from Publix only: $%5.2f\n', apublix);
                    choice3 = menu('Please enter tax %','5%','6%','7%','done');
                    switch choice3
                        case 1
                            apublix = apublix + (apublix*0.05);
                            disp('Total (including 5% tax):');
