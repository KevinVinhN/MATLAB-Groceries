                        case 4
                            loop2 = OFF;
                            close;
                    end
                case 3
                    fprintf('Subtotal of items strictly Kroger only: $%5.2f\n', akroger);
                    choice3 = menu('Please enter tax %','5%','6%','7%','done');
                    switch choice3
                        case 1
                            akroger = akroger + (akroger*0.05);
